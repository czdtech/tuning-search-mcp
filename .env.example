# TuningSearch MCP Server Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Your TuningSearch API key
TUNINGSEARCH_API_KEY=your_api_key_here

# Optional: API base URL (default: https://api.tuningsearch.com/v1)
TUNINGSEARCH_BASE_URL=https://api.tuningsearch.com/v1

# Optional: Request timeout in milliseconds (default: 30000)
TUNINGSEARCH_TIMEOUT=30000

# Optional: Number of retry attempts (default: 3)
TUNINGSEARCH_RETRY_ATTEMPTS=3

# Optional: Retry delay in milliseconds (default: 1000)
TUNINGSEARCH_RETRY_DELAY=1000

# Optional: Log level (debug, info, warn, error) (default: info)
TUNINGSEARCH_LOG_LEVEL=info

# Optional: Log format (json, text) (default: json)
TUNINGSEARCH_LOG_FORMAT=json